{
  "manifest_version": 3,
  "name": "Zero Inbox",
  "description": "Take a look at your email analytics and easily erase unnecessary emails",
  "version": "1.0",

  "page_action": {
    "default_popup": "index.html", 
    "default_icon": {
      "128": "icon.png",
      "48": "icon.png",
      "16": "icon.png"
      }
  },
  
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://mail.google.com/*"
  ]
}
