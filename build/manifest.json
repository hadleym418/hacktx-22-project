{
  "manifest_version": 3,
  "name": "Zero Inbox",
  "description": "NOT GWC Chrome Extension Starter Kit",
  "version": "1.0",

  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://mail.google.com/*"
  ]
}
